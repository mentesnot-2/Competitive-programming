def countSwaps(a):
    cswap=0
    for i in range(n):
        for j in range(n-i-1):
            if a[j]>a[j+1]:
                temp=a[j]
                a[j]=a[j+1]
                a[j+1]=temp
                cswap+=1
    print("Array is sorted in {} swaps.".format(cswap))
    print("First Element: " + str(a[0]))
    print("Last Element: " + str(a[-1]))
   

if __name__ == '__main__':
    n = int(input().strip())

    a = list(map(int, input().rstrip().split()))

    countSwaps(a)
